/* REFACTOR OF PROJECT 6 FOR USART6, AND USING UART TO USB CONVERTER!
 * Port C - PC6 RX / PC7 TX / PC8 [AF8]
 */

#include "stm32f7xx.h"
#include <stdio.h>
#include "uart.h"
#include "led.h"



int main (void)
{
	int x;
	uart6_rxtx_init();

	while (1)
	{
		printf("Testing UART!!\n\r");

		for (int i = 0; i < 9000; i++)
		{
			x++;
		}
	}
}












